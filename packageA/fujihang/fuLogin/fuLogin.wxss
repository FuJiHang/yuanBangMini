#fuLogin{overflow:hidden;height:100vh;width:100vw}#fuLogin .bg{height:1rpx;width:100%}#fuLogin .contant{position:fixed;top:0;left:0;width:100%;height:100%;z-index:5;display:flex;flex-direction:column;justify-content:center;text-align:center;font-size:32rpx;color:#ffff;font-weight:500}#fuLogin .contant .van-button__text{display:flex;align-items:center}#fuLogin .contant .van-button{overflow:hidden;display:flex;align-items:center;border-radius:50rpx;font-weight:500;margin:0 auto;width:340rpx;color:#559eff;border:1px solid #559eff}#fuLogin .contant .van-button image{height:1rpx;width:48rpx;margin-right:20rpx}#fuLogin .child{margin-bottom:50rpx;font-weight:500;animation:mymove .8s}.van-popup{background:none !important}.alertFu{display:flex;align-items:center;flex-direction:column}.alertFu .alert{width:calc(100vw - 200rpx);padding:52rpx 36rpx 54rpx;border-radius:40rpx;background:#fff}.alertFu .alert .nav{font-size:52rpx;color:#559EFF;font-weight:500}.alertFu .post{display:flex;align-items:center;box-shadow:0 1px 5px 0 rgba(160,187,225,0.6);border-radius:46rpx;height:92rpx;padding:0 20rpx;overflow:hidden;margin-top:48rpx}.alertFu .post .input{flex:1;margin-right:20rpx}.alertFu .post .input input{padding-left:10rpx;font-size:32rpx;width:100%}.alertFu .post .van-button{color:#CCCCCC;font-size:28rpx;padding:0;margin:0;border:none;border-left:1px solid #cfcfcf;height:40rpx;padding-left:20rpx}.alertFu .red .van-button{color:#FC554F}.alertFu .btn{margin:64rpx auto 0;display:flex;justify-content:center}.alertFu .btn .van-button{width:430rpx;height:96rpx;background:#ccc;border-radius:48rpx;font-size:32rpx;font-weight:500;color:#fff}.alertFu .blue .van-button{background:#559EFF}.alertFu .close{margin-top:90rpx}.alertFu .close image{height:1rpx;width:78rpx}@keyframes mymove{from{transform:translateY(300rpx);opacity:.3}to{transform:translateY(0);opacity:1}}